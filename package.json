{
  "name": "jamf-mcp-agent",
  "version": "1.0.0",
  "description": "Proactive IT admin agent that monitors fleet health via Jamf Pro MCP server and reports to Slack",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "jamf-agent": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "cli": "tsx src/cli/index.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "jamf",
    "mcp",
    "agent",
    "fleet-management",
    "slack"
  ],
  "author": "dbanks",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dbankscard/jamf-mcp-agent.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.750.0",
    "@aws-sdk/client-secrets-manager": "^3.985.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@slack/web-api": "^7.8.0",
    "aws-embedded-metrics": "^4.2.1",
    "commander": "^13.1.0",
    "dotenv": "^16.6.1",
    "node-cron": "^3.0.3",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/node-cron": "^3.0.11",
    "tsx": "^4.0.0",
    "typescript": "^5.8.0",
    "vitest": "^4.0.18"
  }
}
